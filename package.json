{
  "name": "gatsby-remark-sub-sup",
  "version": "1.0.0",
  "description": "Custom syntax to parse subscript and superscript. Rehype compatible (using <sub> and <sup>)",
  "main": "index.js",
  "scripts": {
    "build": "babel --out-dir . --ignore __tests__ src",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "test": "jest --coverage"
  },
  "private": false,
  "repository": "https://github.com/winkey728/gatsby-remark-sub-sup.git",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "markdown"
  ],
  "author": "Winkey Wong <winkeyme@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/winkey728/gatsby-remark-sub-sup/issues"
  },
  "homepage": "https://github.com/winkey728/gatsby-remark-sub-sup#readme",
  "files": [
    "index.js",
    "README.md"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "dedent": "^0.7.0",
    "jest": "^22.4.3",
    "remark": "^9.0.0",
    "unist-util-find": "^1.0.1"
  },
  "dependencies": {
    "remark-sub-super": "^1.0.11"
  }
}
